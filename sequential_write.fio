; fio job file
; http://git.kernel.dk/?p=fio.git
;
; Sequential write pattern -jdc

[global]
name=writer
description=Sequential write pattern
directory=/Volumes/External
rw=write
ioengine=posixaio
iodepth=4
direct=0
size=1g
runtime=1m
time_based
write_bw_log
write_lat_log
thread

[sequential-write-4k]
stonewall
bs=4k

[sequential-write-16k]
stonewall
bs=16k

[sequential-write-64k]
stonewall
bs=64k
